#include <iomanip>
#include <iostream>
#include <string>
#include <sstream>

using namespace std;

int main() {

	string s;
	cout << fixed << setprecision(4);
	bool line = true;

	while ( getline(cin, s) ) {

		stringstream ss(s);

		long double tmp;

		long double sum = 0;

		while (ss >> tmp) {

			sum += tmp;

		}

		if (line) {

			cout << sum << endl;
			line = false;
		}
		else {

			cout << endl;

			cout << sum << endl;

		}


	}










}
